from pygame import *

BLUE = (200, 255, 255)
win_width = 600
win_height = 500
win = display.set_mode(win_width, win_height)
win.fill(BLUE)

clock = time.Clock()
FPS = 60
game = True

while game:
    for e in event.get():
        if e.type = QUIT:
            game = True
    display.update()
    clock.tick(FPS)
